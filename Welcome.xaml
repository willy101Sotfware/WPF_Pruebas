<Window x:Class="WPF_Pruebas.Welcome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Pruebas"
        mc:Ignorable="d"
        Title="Welcome"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        WindowStyle="None"
        WindowState="Maximized">
    
    <Grid MouseDown="Continuar_Touch">
        <Viewbox x:Name="WindowVB" Stretch="Uniform">
            <ContentControl x:Name="MainContainer" Width="1080" Height="1920">
                <Image Source="{StaticResource BACKWELCOME}" Stretch="Fill"/>
            </ContentControl>
        </Viewbox>
    </Grid>
</Window>
